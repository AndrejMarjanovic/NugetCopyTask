  <!-- Web.Config transformation after  -->
  <Target Name="TransformCompanyWebConfig" AfterTargets="TransformWebConfig" Condition="'$(Company)' != '' AND exists('Configuration\$(Configuration)\$(Company)\Web.config')">
    <!-- Generate transformed Connection Strings config in the intermediate directory -->
    <Warning Text="Transforming Web Config With Company Specific data. Source file: Configuration\$(Configuration)\$(Company)\Web.config" />
    <TransformXml Source="$(IntermediateOutputPath)\TransformWebConfig\transformed\Web.config" Destination="$(IntermediateOutputPath)\TransformWebConfig\transformed\Web.config" Transform="Configuration\$(Configuration)\$(Company)\Web.config" />
  </Target>
  <!-- AppSettings -->
  <Target Name="TransformAppSettingsConfig" AfterTargets="PrintCompanyName" Condition="'$(Company)' != '' AND exists('Configuration\$(Configuration)\$(Company)\appSettings.config')">
    <!-- Generate transformed AppSettings config in the intermediate directory -->
    <Warning Text="Transforming App Settings With Company Specific data. Source file: Configuration\$(Configuration)\$(Company)\appSettings.config" />
    <TransformXml Source="Configuration\$(Configuration)\appSettings.config" Destination="Configuration\$(Configuration)\appSettings.config" Transform="Configuration\$(Configuration)\$(Company)\appSettings.config" />
  </Target>
